
public class buzzz {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		        for (int i = 1; i <= 100; i++) {
		            if (i % 3 == 0 && i % 5 == 0) {
		                System.out.println("%d: fizz buzz"+i);
		            } else if (i % 5 == 0) {
		                System.out.println("%d: buzz"+i);
		            } else if (i % 3 == 0) {
		                System.out.println("%d: fizz"+i);
		            } 
		        }
		        System.out.println("\n");
		    }
	



}
