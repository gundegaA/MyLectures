
public class PrimType {

	
	public static void main(String[] args) { 
		//numeric  data primite type
		int integer = 2;
		long longInteger = 202020202020L;
		short shortInteger = 9000;
		byte byteInteger = 120;
		
		//Floating -point type real numbers
		double doubleFloutNumber = 0.5d;
		float floatNumber = 0.123455554f;
		
		//characters
		char charSymbol= 'a';
		char charSymbolAsNumber = 99;
		
		System.out.println(charSymbolAsNumber);

	}

}
