
public class ArraysSumForLoop {

	
	public static void main(String[] args) {
		
		int[] marks= {45,76,87,34,66,87,100};
		
		//sum all values in array
		int sum=0;
		int sum1=0;
		
		for (int i = 0; i<marks.length; i++){
			sum= sum+ marks[i];
			sum1+=marks[i];
			
		}
		System.out.println("Sum is "+sum+" or "+sum1);

	}

}
