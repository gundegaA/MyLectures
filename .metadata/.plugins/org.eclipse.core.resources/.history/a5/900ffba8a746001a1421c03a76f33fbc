
public class Primes {

	
	public static void main(String[] args) {
		
		int n =10;
		
		for(int i=3;i<=n/2+1;i++){
			int sk =0;
			for (int j=2;j<i;j++){
				
				
				if (i%j==0){
					sk =sk+1;
				}else{
					sk =sk+0;
				}
			}
			if (sk==0){
				System.out.println("prime is: "+i);
			}
			
			
			
			
		}
		

	}

}
