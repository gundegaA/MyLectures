
public class TaskVar {

	
	public static void main(String[] args) {
		
		int a=125;
		int b=24;
			System.out.println(a+"+"+b+"="+(a+b));
			System.out.println(a+"-"+b+"="+(a-b));
			System.out.println(a+"x"+b+"="+(a*b));
			System.out.println(a+"/"+b+"="+(a/b));
			System.out.println(a+" mod "+b+"="+(a % b));
		

			
			/**
			 * 125 + 24 = 149
			 *
			*125 - 24 = 101
			*125 x 24 = 3000
			125 / 24 = 5
			125 mod 24 = 5
			*/
			
			
	}

}
