		double num1 = 4;
		double num2 = 7;
		char c = '+';
				switch(c){
				case '+':
					System.out.println(""+num1 + c +num2+" = " +(num1 + num2));
					break;
				case 2:
					System.out.println("TWO");
					break;
				case 3:
					System.out.println("THREE");
					break;
				default:
					System.out.println("OTHER");
	}

}
