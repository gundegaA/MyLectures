
public class ArraysPrimitiveDataType {

	
	public static void main(String[] args) {
		
		int[] arrays;
		//int arrays2[];
		arrays = new int[2];
		
		//VAI
		
		int[] arr =new int[11];
		
		int[] arrTest = new int[2];
		
		arrTest[0] = 10;
		arrTest[1] = 8;
		
		System.out.println(arrTest[0]+" "+arrTest[1]);
		

	}

}
