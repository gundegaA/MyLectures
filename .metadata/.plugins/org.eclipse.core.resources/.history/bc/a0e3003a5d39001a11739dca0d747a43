
public class ControlFlowIf {

	
	public static void main(String[] args) {
		
		//REMAINDER
		int x=101;
		int y=22;
		
		if(x%2==0){
			System.out.println(x + " ir para skaitlis.");
		}
		else {
			System.out.println(x + " ir nepara skaitlis");
		}
		
		
		//PASS FAIL
			int mark=22;
		
		if(mark>=50){
			System.out.println("PASS");
		}
		else {
			System.out.println("FAIL");
		}
		System.out.println("DONE");
	}

}