		cars.add("BMW");
		cars.add("FORD");
		cars.add("MAZDA");
		cars.add("WV");
		cars.add("HONDA");
		cars.add("HUNDAI");
		cars.add("TOYOTA");
		cars.add("VOLVO");
		cars.add("KIA");
		
		System.out.println(cars);
		System.out.println(cars.size()); //lielums
		

	}

}
