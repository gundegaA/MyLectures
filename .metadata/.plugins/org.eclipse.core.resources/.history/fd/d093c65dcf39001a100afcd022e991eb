
public class PrintNumberInWorld {

	
	public static void main(String[] args) {
		
		int number = 1;
		
		if (number == 1){
			System.out.println("ONE");
		}else if (number == 2){
			System.out.println("TWO");
		}else if (number ==3){
			System.out.println("THREE");
		}
		else {
			System.out.println("OTHER");
		}

	}

}
